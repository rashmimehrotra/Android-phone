<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TodayScheduleActivity"
    android:background="#fff">

    <com.app.learniatstudent.CalenderView.WeekView
        android:id="@+id/weekView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#244463"
        app:futureBackgroundColor="#244463"
        app:pastBackgroundColor="#244463"
        app:eventTextColor="@android:color/white"
        app:textSize="12sp"
        app:hourHeight="60dp"
        app:headerColumnPadding="8dp"
        app:headerColumnTextColor="#ffffff"
        app:headerRowPadding="10dp"
        app:columnGap="1dp"
        app:noOfVisibleDays="1"
        app:headerRowBackgroundColor="#244463"
        app:dayBackgroundColor="#05000000"
        app:todayBackgroundColor="#1848adff"
        app:headerColumnBackground="#244463"
        app:todayHeaderTextColor="@color/accent"/>

</RelativeLayout>
